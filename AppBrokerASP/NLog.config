<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <targets>
    <target name="logfile" xsi:type="File" fileName="file.txt" />
    <target name="file" xsi:type="File" fileName="${basedir}/Logs/${shortdate}.log" />
    <target name="logconsole" xsi:type="Console" />
  </targets>

  <rules>
      <logger name="Microsoft.EntityFramework*" maxlevel="off" final="true"/>
      <logger name="Elsa.*" maxlevel="off" final="true"/>
      <logger name="Rebus.*" maxlevel="off" final="true"/>
      <logger name="Quartz.*" maxlevel="off" final="true"/>
      <logger name="*" minlevel="debug" writeTo="logconsole" />
      <logger name="*" minlevel="debug" writeTo="file" />
  </rules>
</nlog>